{
	// Workspace
	"workbench.colorTheme": "Monokai Vibrant",
	"workbench.activityBar.visible": false,
	"window.zoomLevel": 0,
	"editor.fontSize": 11,
	"editor.renderWhitespace": "none",
	// OS Specific
	"window.menuBarVisibility": "toggle",
	"terminal.integrated.shell.windows": "C:\\Windows\\System32\\cmd.exe",
	// Standard
	"telemetry.enableTelemetry": false,
	"telemetry.enableCrashReporter": false,
	"workbench.startupEditor": "none",
	"workbench.editor.untitled.hint": "hidden",
	"files.trimTrailingWhitespace": false,
	"editor.fontLigatures": true,
	"editor.accessibilitySupport": "off",
	"editor.multiCursorModifier": "ctrlCmd",
	"editor.minimap.enabled": false,
	"editor.wordWrap": "on",
	"editor.links": false,
	"editor.insertSpaces": false,
	"editor.tabCompletion": "on",
	"editor.suggest.snippetsPreventQuickSuggestions": false,
	"editor.quickSuggestions": true,
	"editor.autoClosingBrackets": "always",
	"editor.detectIndentation": false,
	// "editor.suggest.maxVisibleSuggestions": 15,
	"explorer.openEditors.visible": 0,
	"extensions.ignoreRecommendations": true,
	"extensions.showRecommendationsOnlyOnDemand": true,
	"terminal.integrated.fontSize": 11,
	"terminal.integrated.cursorStyle": "line",
	"terminal.integrated.cursorBlinking": true,
	"terminal.integrated.scrollback": 5000,
	"terminal.integrated.tabs.enabled": false,
	"search.searchOnTypeDebouncePeriod": 175,
	"breadcrumbs.enabled": false,
	"window.title": "${dirty}${activeEditorMedium}${separator}${rootName}${separator}${appName}",
	// Addons
	"python.showStartPage": false,
	"sqltools.useNodeRuntime": true, // Must install nodejs
	"sqltools.highlightQuery": false,
	"rust-analyzer.lens.enable": false,
	"rust-analyzer.inlayHints.enable": false,
	"code-runner.enableAppInsights": false,
	"code-runner.respectShebang": false,
	"code-runner.ignoreSelection": true,
	"code-runner.showRunIconInEditorTitleMenu": false,
	"code-runner.clearPreviousOutput": true,
	"code-runner.runInTerminal": true,
	"platformio-ide.disableToolbar": true,
	"platformio-ide.disablePIOHomeStartup": true,
	//"C_Cpp.updateChannel": "Insiders",
	// Languages
	"files.exclude": {
		"**/env": true,
		"**/venv": true
	},
	"search.exclude": {
		"**/env": true,
		"**/venv": true
	},
	"code-runner.executorMap": {
		"rust": "cargo run"
	},
	"[rust]": {
		"editor.semanticHighlighting.enabled": false
	},
	"[sql]": {
		"editor.defaultFormatter": "adpyke.vscode-sql-formatter"
	},
	"[html]": {
		"editor.tabSize": 3
	},
	"[jinja-html]": {
		"editor.tabSize": 3
	},
	// What is going on here...?
	"workbench.editorAssociations": {
		"*.ipynb": "jupyter-notebook"
	},
	"notebook.cellToolbarLocation": {
		"default": "right",
		"jupyter-notebook": "left"
	},
}